number1 = input("Enter the limi 1: ")
number1 = int(number1)
number2 = input("Enter the limi 2: ")
number2 = int(number2)

if number1< number2:
    while number1 <= number2:
        print(number1)
        number1 += 1
elif number1 > number2:
    while number2 <= number1:
        print(number2)
        number2 += 1
