number1 = input("Enter the limi 1: ")
number1 = int(number1)
number2 = input("Enter the limi 2: ")
number2 = int(number2)

if number1< number2:
    while number1 <= number2:
        if number1%2 == 0:
            print(number1," - even")
        else:
            print(number1," - odd")
        number1 += 1
elif number1 > number2:
    while number2 <= number1:
        if number2%2 == 0:
            print(number2," - even")
        else:
            print(number2," - odd")
        number2 += 1
